x = float(input())
if 969.5 >= x >= 0.01:
    y = ((x - 0.01) * 0.0192)
elif 1703.8 >= x >= 969.51:
    y = ((x - 969.51) * 0.1088 + 56.91)
elif 4782.61 >= x >= 1703.81:
    y = ((x - 1703.81) * 0.16 + 136.85)
elif 14391.44 >= x >= 4782.62:
    y = ((x - 4782.62) * 0.2352 + 766.15)
elif 19188.61 >= x >= 14391.45:
    y = ((x - 14391.45) * 0.32 + 3470.25)
elif 57565.76 >= x >= 19188.62:
    y = ((x - 19188.62) * 0.34 + 5005.35)
else:
    y = ((x - 57565.77) * 0.35 + 18053.63)
print(y)
